# NAJD Backend Environment Variables
POSTGRESQL_DB_HOST=localhost
POSTGRESQL_DB_USER=postgres
POSTGRESQL_DB_PASSWORD=<database_password>
POSTGRESQL_DB_NAME=najd

# --- Required: Security Settings ---
# IMPORTANT: Generate a new, strong secret key for production.
# You can generate one with: openssl rand -hex 32
SECRET_KEY=<your_strong_random_secret_key>

# --- Required: Agent API Keys ---
# API keys for the services used by the agent.
OPENAI_API_KEY=<your_openai_api_key>
TAVILY_API_KEY=<your_tavily_api_key>

# --- Optional: Daytona Sandbox Settings ---
# API key is only required if you are using a private Daytona instance.
# DAYTONA_API_KEY=

# --- Optional: Overwrite Default Values ---
# The following variables have default values set in the code.
# You can uncomment and change them here if needed.
#
# POSTGRESQL_DB_PORT=5432
#
# DAYTONA_API_URL=https://app.daytona.io/api
# DAYTONA_TARGET=us
# DAYTONA_CONTAINER_IMAGE=kortix/suna:0.1.3
#
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRY_DAYS=120
AGENT_VERSION_ROUTER_API=/v1

# Steel
STEEL_BROWSER_SECRET_KEY = <your_steel_browser_key>

#Google
GOOGLE_WORKSPACE_MCP_URL = <>
GOOGLE_OAUTH_CLIENT_ID = <>
GOOGLE_OAUTH_CLIENT_SECRET = <>
GOOGLE_OAUTH_REDIRECT_URI = <>
